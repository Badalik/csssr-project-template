.select
	padding-right 40px
	padding-left 20px
	width 100%
	height 60px
	font-size 16px
	color #777
	background-color #fff
	background-image url(../images/select-arrow.svg)
	background-repeat no-repeat
	background-position right 20px center
	background-size 16px 10px
	border-width 1px
	border-style solid
	border-color #dadfe9
	border-radius 100px
	box-shadow none
	appearance none
	transition .2s border-color ease-out 0s
	backface-visibility hidden
	will-change border-color
